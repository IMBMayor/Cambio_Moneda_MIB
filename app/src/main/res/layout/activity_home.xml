<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="190dp">

        <TextView
            android:id="@+id/Usuario"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:editable="false"
            android:textAlignment="center"
            android:paddingTop="10dp"
            />

        <Button
            android:id="@+id/GEO_Cambio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="Geo_Cambio"
            android:drawableRight="@drawable/ic_baseline_map_24"
            android:text="GEO Cambio" />

        <Button
            android:id="@+id/Manual_Cambio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="Manual_Cambio"
            android:drawableRight="@drawable/ic_baseline_monetization_on_24"
            android:text="Cambio Manual" />

        <Button
            android:id="@+id/Historial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="Historial"
            android:drawableRight="@drawable/ic_baseline_history_24"
            android:text="Historial" />

        <Button
            android:id="@+id/Perfil"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="Perfil"
            android:drawableRight="@drawable/ic_baseline_person_24"
            android:text="Perfil" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>